        android:ems="10"
        android:enabled="false"
        android:hint="@string/editText_Hint_Apellidos"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        tools:ignore="VisualLintBounds,DuplicateSpeakableTextCheck" />/>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="292dp"
        android:text="@string/textView_ParteEntera"
        tools:ignore="VisualLintBounds,TextContrastCheck" />/>

    <Button
        android:id="@+id/button_resultados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="112dp"
        android:layout_marginTop="564dp"
        android:text="@string/button_resultado"
        tools:ignore="TouchTargetSizeCheck,VisualLintBounds" />/>

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="104dp"
        android:text="@string/textView_Apellidos"
        tools:ignore="VisualLintBounds,TextContrastCheck" />/>
</RelativeLayout>